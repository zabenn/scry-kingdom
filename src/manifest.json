{
  "manifest_version": 2,
  "name": "Firefox Extenion Template",
  "version": "1.0",
  "description": "A TypeScript-ready Firefox extension template",
  "permissions": ["https://api.scryfall.com/*"],
  "content_scripts": [
    {
      "matches": ["https://scryfall.com/card/*"],
      "js": ["contentScript.js"]
    }
  ]
}
